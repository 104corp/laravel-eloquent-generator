{
    "name": "104corp/eloquent-generator",
    "description": "Laravel Eloquent Generator",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/104corp/laravel-eloquent-generator",
    "support": {
        "issues": "https://github.com/104corp/laravel-eloquent-generator/issues"
    },
    "require": {
        "laravel/framework": "~5.1",
        "xethron/migrations-generator": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 <6",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Corp104\\Eloquent\\Generator\\": "src"
        }
    },
    "bin": [
        "eg"
    ],
    "config": {
        "sort-packages": true
    }
}
